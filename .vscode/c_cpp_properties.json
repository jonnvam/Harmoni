{
    "version": 4,
    "configurations": [
        {
            "name": "Windows Flutter",
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-msvc-x64",
            "includePath": [
                "${workspaceFolder}/windows",
                "${workspaceFolder}/windows/flutter/ephemeral/include",
                "${workspaceFolder}/windows/flutter/ephemeral/.plugin_symlinks/**/windows/include"
